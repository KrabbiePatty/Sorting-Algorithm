/*compare funoaction
  */

func compare(Left:String, Right:String) -> Bool {
    if Left < Right {
        return true
    } else if Left > Right {
        return false
    } else if Left == Right {
        return true
    } else {
        return true
    }
}

print(compare(Left:"apple", Right: "banana"))
